{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier", "jest"],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "prettier/prettier": ["error"],
        "indent": ["error", 4],
        "max-len": "off"
    }
}
