{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "max-len": "off"
    }
}
